        <script>

           


            // global variables
            var vid1 = document.getElementById('TFSA')
            var vid2 = document.getElementById('overcontributing').removeAttribute('autoplay')
            var vid3 = document.getElementById('dividend').removeAttribute('autoplay')
            
            // var vid4 =
            // var vid5 = 
            // var vid6 = 
            // var vid7 = 
            // var vid8 = 
            // var vid9 = 
            // var vid10= 

            function functionList(){
            // to close All tabs 
            aria-expanded===false
               if (document.getElementById('video').ontoggle = true){
                document.getElementById('List1').classList.add("visually-hidden")
               }
            }
            

            //TFSA
            function functionA() {
                document.querySelector('.RRSP').classList.add('RRSPSlides')
                document.querySelector('.RESP').classList.add('RESPSlides')
                document.querySelector('.TFSA').classList.remove('TFSASlides')
                // document.getElementById('List1').setAttribute("visibility, visible")
             
            }

           function function1(){
                document.getElementById('Mistakes').classList.remove('active')
                document.getElementById('overcontributing').classList.remove('active')
                document.getElementById('TFSA').classList.add('active')
                document.getElementById('dividend').classList.remove('active')
                // vid1.style.display = vid1.style.display === 'none'? '' : 'none';
                
            
           }

           function function2(){
                document.getElementById('Mistakes').classList.add('active')
                document.getElementById('overcontributing').classList.remove('active')
                document.getElementById('TFSA').classList.remove('active')
                document.getElementById('dividend').classList.remove('active')
           }

           function function3(){
                document.getElementById('Mistakes').classList.remove('active')
                document.getElementById('overcontributing').classList.add('active')
                document.getElementById('TFSA').classList.remove('active')
                document.getElementById('dividend').classList.remove('active')
           }

           function function4(){
                document.getElementById('Mistakes').classList.remove('active')
                document.getElementById('overcontributing').classList.remove('active')
                document.getElementById('TFSA').classList.remove('active')
                document.getElementById('dividend').classList.add('active')
           }

        // RRSP 
            function functionB() {
                document.querySelector('.TFSA').classList.add('TFSASlides')
                document.querySelector('.RESP').classList.add('RESPSlides')
                document.querySelector('.RRSP').classList.remove('RRSPSlides')
            
            }

            function function5(){  
                document.getElementById('RRSP').classList.add('active')
                document.getElementById('lifeLong').classList.remove('active')
                document.getElementById('homeBuyer').classList.remove('active')
                document.getElementById('mistakes').classList.remove('active')
            }

            function function6 (){
                document.getElementById('RRSP').classList.remove('active')
                document.getElementById('lifeLong').classList.remove('active')
                document.getElementById('homeBuyer').classList.remove('active')
                document.getElementById('mistakes').classList.add('active')
            }

            function function7(){
                document.getElementById('RRSP').classList.remove('active')
                document.getElementById('lifeLong').classList.add('active')
                document.getElementById('homeBuyer').classList.remove('active')
                document.getElementById('mistakes').classList.remove('active')
            }
            
            function function8(){
                document.getElementById('RRSP').classList.remove('active')
                document.getElementById('lifeLong').classList.remove('active')
                document.getElementById('homeBuyer').classList.add('active')
                document.getElementById('mistakes').classList.remove('active')
            }

        // RESP
         
         
            function functionC() {
                document.querySelector('.TFSA').classList.add('TFSASlides')
                document.querySelector('.RRSP').classList.add('RRSPSlides')
                document.querySelector('.RESP').classList.remove('RESPSlides')
                document.getElementById('CESG').classList.remove('active')
                document.getElementById('RESP').classList.add('active')
            }
           
            function function9(){
                document.getElementById('CESG').classList.remove('active')
                document.getElementById('RESP').classList.add('active')
            }

            function function10(){
                document.getElementById('CESG').classList.add('active')
                document.getElementById('RESP').classList.remove('active')
            }

